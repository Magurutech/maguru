{
  "name": "maguru",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "app": "cross-env APP_ENV=dev npx next dev --turbopack",
    "app:prod": "cross-env APP_ENV=prod npx next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "build:css": "tailwindcss -i src/styles/globals.css -o public/css/output.css --minify",
    "test:db": "tsx scripts/test-db.ts",
    "test": "jest app/creator/dashboard/page.test.tsx  --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{ts,tsx}' --collectCoverageFrom='!**/*.d.ts' --collectCoverageFrom='!**/node_modules/**'",
    "test:unit": "jest features/course/ --verbose --passWithNoTests",
    "test:unit:all": "jest features/ --verbose --passWithNoTests",
    "test:integration": "jest __tests__/integration/course/ --verbose --passWithNoTests",
    "test:integration:all": "jest __tests__/integration --verbose --passWithNoTests",
    "test:e2e": "cross-env NODE_ENV=test  playwright test __tests__/playwright/course/",
    "test:e2e:all": "cross-env NODE_ENV=test playwright test __tests__/playwright/",
    "test:e2e:ui": "cross-env NODE_ENV=test  playwright test --ui",
    "test:e2e:debug": "cross-env NODE_ENV=test  playwright test __tests__/playwright/auth/sign-in.spec.ts --debug",
    "test:e2e:report": "playwright show-report services/playwright-report",
    "env:validate": "tsx -e \"const { validateEnvSafe } = require('./lib/env-validation.ts'); const result = validateEnvSafe(); if (!result.success) { console.error(result.error); process.exit(1); } console.log('âœ… Environment validation passed');\""
  },
  "dependencies": {
    "@clerk/nextjs": "^6.23.0",
    "@prisma/client": "^6.10.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@testing-library/react-hooks": "^8.0.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "lucide-react": "^0.523.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-test-renderer": "^19.1.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@clerk/testing": "^1.9.1",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-config-next": "15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "msw": "^2.10.2",
    "node-fetch": "2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "web-streams-polyfill": "^4.1.0"
  }
}
