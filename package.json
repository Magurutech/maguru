{
  "name": "maguru",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "app": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:css": "tailwindcss -i src/styles/globals.css -o public/css/output.css --minify",
    "test:db": "tsx scripts/test-db.ts",
    "test": "jest app/creator/dashboard/page.test.tsx  --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/*.{ts,tsx}' --collectCoverageFrom='!**/*.d.ts' --collectCoverageFrom='!**/node_modules/**'",
    "test:custom": "jest features/homepage/component/Feature.test.tsx features/homepage/component/Footers.test.tsx  features/homepage/component/Heros.test.tsx features/homepage/component/Navbars.test.tsx features/homepage/component/Stat.test.tsx features/homepage/component/Testimonial.test.tsx --verbose",
    "test:unit": "jest features/auth --verbose --passWithNoTests",
    "test:integration": "jest __tests__/auth/integration --verbose --passWithNoTests",
    "test:e2e": "playwright test",
    "test:all": "npm run test:unit && npm run test:integration",
    "env:validate": "tsx -e \"const { validateEnvSafe } = require('./lib/env-validation.ts'); const result = validateEnvSafe(); if (!result.success) { console.error(result.error); process.exit(1); } console.log('âœ… Environment validation passed');\"",
    "prepare": "npm run env:validate"
  },
